    "curd rice": {
        "name": "Curd Rice",
        "ingredients": [
            "1 cup cooked rice",
            "1/2 cup curd",
            "Salt to taste",
            "1 tsp mustard seeds",
            "1-2 green chilies, chopped",
            "1 tsp grated ginger",
            "1 tbsp oil",
            "A few curry leaves",
            "Coriander leaves for garnish"
        ],
        "steps": [
            "Cook rice and let it cool.",
            "Add curd and salt to the rice and mix well.",
            "Heat oil in a pan, add mustard seeds, let them splutter.",
            "Add chopped green chilies, ginger, and curry leaves. Saut\u00e9 for a few seconds.",
            "Add the tempering to the rice-curd mix.",
            "Garnish with coriander leaves and serve chilled."
        ],
